reset
set xr <%= range %>
set yr [*:*]
set ylabel "<%= @ylabel %>"
set xlabel "<%= @xlabel %>"
set nokey
set notitle
set size 0.6,0.6
set term postscript eps mono
set out "<%= @output %>"
plot <% @plots.each do |plot| %> \
       '<%= plot.input %>' using ($1):($2) t "" <% if plot.with %> w <%= plot.with %> <% end %>
     <% end %>
